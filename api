curl -X POST http://localhost:5001/ \
  -H "Content-Type: text/xml" \
  -d '<?xml version="1.0"?>
<soap:Envelope xmlns:soap="http://schemas.xmlsoap.org/soap/envelope/" xmlns:calc="http://calculator.example.com/">
  <soap:Body>
    <calc:multiply>
      <calc:a>7</calc:a>
      <calc:b>6</calc:b>
    </calc:multiply>
  </soap:Body>
</soap:Envelope>'
